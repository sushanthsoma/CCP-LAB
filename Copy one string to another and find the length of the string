#include<stdio.h>
    int main()
    {
    char str1[100],str2[100];
    int j=0;
    printf("Enter String 1: ");
    scanf("%s",str1);
    for(int i=0;str1[i]!='\0';i++)

    {
    str2[i]=str1[i];
    j++;
            }

    printf("String s2 is: %s\n",str2);
    printf("The length of the string is: %d",j);
    return 0;
    }
