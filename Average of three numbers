#include<stdio.h>
#include<math.h>

float average(int,int,int);
int main()
{
   int num1,num2,num3;
   float avg;
   printf("\n ENTER 3 NUMBERS=");
   scanf("%d %d %d",&num1,&num2,&num3);
   avg = average(num1,num2,num3);
   printf("\n AVERAGE=%.2f \n",avg);
   return 0;
}
float average(int n1,int n2,int n3)
{
    float s;
    s=((float)n1+(float)n2+(float)n3)/3;
    return(s);
}
